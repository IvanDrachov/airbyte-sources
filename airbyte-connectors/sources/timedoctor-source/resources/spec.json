{
  "documentationUrl": "https://docs.faros.ai",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "TimedoctorV2 Spec",
    "type": "object",
    "required": ["server_url", "token"],
    "additionalProperties": true,
    "properties": {
      "server_url": {
        "type": "string",
        "title": "TimedoctorV2 API Server URL",
        "examples": ["https://api2.timedoctor.com/api/1.0/"]
      },
      "token": {
        "type": "string",
        "title": "token",
        "airbyte_secret": true
      }
    }
  }
}